{
  "name": "office-addin-taskpane-react",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/OfficeDev/Office-Addin-TaskPane-React.git"
  },
  "license": "MIT",
  "config": {
    "app_to_debug": "outlook",
    "app_type_to_debug": "desktop",
    "dev_server_port": 3000
  },
  "scripts": {
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "dev-server": "set NODE_ENV=development && webpack serve --mode development",
    "lint": "office-addin-lint check",
    "lint:fix": "office-addin-lint fix",
    "prettier": "office-addin-lint prettier",
    "start": "set NODE_ENV=development && office-addin-debugging start manifest.local.xml",
    "start:desktop": "set NODE_ENV=development && office-addin-debugging start manifest.local.xml desktop",
    "start:web": "office-addin-debugging start manifest.xml web",
    "stop": "office-addin-debugging stop manifest.local.xml",
    "validate": "office-addin-manifest validate manifest.xml",
    "watch": "webpack --mode development --watch"
  },
  "dependencies": {
    "@fontsource/mulish": "^5.2.5",
    "assert": "^2.1.0",
    "axios": "^1.8.2",
    "bootstrap": "^5.3.3",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "core-js": "^3.41.0",
    "crypto-js": "^4.2.0",
    "es6-promise": "^4.2.8",
    "jsforce": "^3.6.6",
    "process": "^0.11.10",
    "querystring-es3": "^0.2.1",
    "react": "^19.0.0",
    "react-bootstrap": "^2.10.9",
    "react-bootstrap-typeahead": "^6.4.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-scripts": "5.0.1",
    "react-select": "^5.10.2",
    "react-spinners": "^0.15.0",
    "react-timezone-select": "^3.2.8",
    "regenerator-runtime": "^0.14.1",
    "stream-browserify": "^3.0.0",
    "styled-components": "^6.1.15",
    "swr": "^2.3.3",
    "timers-browserify": "^2.0.12",
    "util": "^0.12.5"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "babel-loader": "^10.0.0",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^7.1.2",
    "eslint-plugin-office-addins": "^4.0.3",
    "eslint-plugin-react": "7.37.4",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.3",
    "office-addin-cli": "^2.0.3",
    "office-addin-debugging": "^6.0.3",
    "office-addin-dev-certs": "^2.0.3",
    "office-addin-lint": "^3.0.3",
    "office-addin-manifest": "^2.0.3",
    "office-addin-prettier-config": "^2.0.1",
    "style-loader": "^4.0.0",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "5.2.0"
  },
  "prettier": "office-addin-prettier-config",
  "browserslist": [
    "ie 11"
  ]
}
